<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <record id="ir_cron_retirement_create" model="ir.cron">
            <field name="name">Hr holiday: create Retirement</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="model_id" ref="hr_employee_info.model_hr_contract"/>
            <field name="code">model.create_employee_retirement()</field>
            <field name="state">code</field>
            <field name="active" eval="True"/>
        </record>
        <record id="ir_cron_get_age" model="ir.cron">
            <field name="name">Hr holiday: Get Employees Age</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="model_id" ref="hr_employee_info.model_hr_employee"/>
            <field name="code">model.compute_employee_birthday()</field>
            <field name="state">code</field>
            <field name="active" eval="True"/>
        </record>
    </data>
</odoo>